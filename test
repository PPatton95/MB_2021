from tsfresh import extract_features
from tsfresh.feature_extraction import extract_features, MinimalFCParameters

def my_extraction(timeseries):
    extracted_features = extract_features(timeseries, column_id="id", column_sort="time", n_jobs=0)
    return extracted_features

my_extraction(timeseries)
